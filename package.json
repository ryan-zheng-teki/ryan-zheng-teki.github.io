    "test": "jest",
    "build": "webpack --config webpack.config.js",
    "start": "cross-env NODE_ENV=develop webpack-dev-server --open --mode development",
    "predeploy": "npm run build",
    "genxml": "folder-to-xml ./src/blogs ./src/config/blog-categories.xml",
